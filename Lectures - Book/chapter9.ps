	    ly 1 hy {
		dp_h mul
		E dup 3 1 roll E
		patmat currentmatrix pop
		TR
		dp_pw dp_ph inv
		dp_mat dp_proc imagemask
		patmat setmatrix
	    } for
	    pop
	} for
} bdef
